{
  imports: [
    layers.hjson
  ]
  enabled: true
  biomes: {
    names: [
      minecraft:desert
      minecraft:desert_hills
      minecraft:mutated_desert
      biomesoplenty:oasis
    ]
  }
  layers: [
    ###########################################
    # UNDERGROUND
    #
    $u_andesite( 0, 30 )
    {
      #Sandstone
      state: minecraft:sandstone
      height: [ 30, 90 ]
      ceiling: $UNDERGROUND
    }
    ###########################################
    # OVERGROUND
    #
    $o_jasper( 90, 150 )
    {
      #Sandstone
      state: minecraft:sandstone
      height: [ 150, 225 ]
      ceiling: $OVERGROUND
    }
    ###########################################
  ]
}